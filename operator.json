{
  "name": "Round",
  "description": "Adjusts cell values (up or down) to a specified base number.",
  "tags": [
    "preprocessing",
    "transformation"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/round_operator"
  ],
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "method",
      "defaultValue": "round",
      "values": [
        "round",
        "floor",
        "ceiling",
        "trunc",
        "signif"
      ],
      "description": "Rounding method"
    },
    {
      "kind": "StringProperty",
      "name": "digits",
      "defaultValue": "0",
      "description": "integer indicating the number of decimal places (round) or significant digits (signif) to be used."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Data",
                "type": "numeric",
                "description": "Input data, per cell",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}