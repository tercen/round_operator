{
  "name": "Round",
  "description": "Adjusts cell values (up or down) to a specified base number.",
  "tags": [
    "preprocessing",
    "transformation"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/round_operator"
  ],
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "method",
      "defaultValue": "round",
      "values": [
        "round",
        "floor",
        "ceiling",
        "trunc",
        "signif"
      ],
      "description": "Rounding method"
    },
    {
      "kind": "StringProperty",
      "name": "digits",
      "defaultValue": "0",
      "description": "integer indicating the number of decimal places (round) or significant digits (signif) to be used."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Optional row factor",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column",
            "type": "",
            "description": "Optional column factor",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Values to round",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "OperatorJoinSpec",
        "joinOperators": [
          {
            "kind": "JoinOperator",
            "joinType": "left",
            "leftPair": {
              "kind": "ColumnPair",
              "lColumns": [
                "Row",
                "Column"
              ],
              "rColumns": [
                "Row",
                "Column"
              ]
            },
            "rightRelation": {
              "kind": "TableRelation",
              "attributes": [
                {
                  "kind": "Attribute",
                  "name": "rounded",
                  "type": "double"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}